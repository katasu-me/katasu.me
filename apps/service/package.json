{
  "name": "@katasu.me/service",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack --experimental-https -H local.katasu.me",
    "build": "next build",
    "build:worker": "opennextjs-cloudflare build",
    "start": "next start",
    "preview": "opennextjs-cloudflare build && opennextjs-cloudflare preview",
    "cf-typegen": "wrangler types -c ./wrangler.jsonc -c ../image-optimizer/wrangler.jsonc --env-interface CloudflareEnv ./cloudflare-env.d.ts",
    "lint": "biome lint --write ./src/**/*",
    "format": "biome format ./src/**/* --write",
    "check": "biome check --write ./src/**/*",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "db:generate": "drizzle-kit generate --dialect sqlite --out ../../packages/service-db/src/migrations --schema ../../packages/service-db/src/schema",
    "db:studio": "dotenv -e .env.local -- drizzle-kit studio",
    "db:migrate": "wrangler d1 migrations apply katasu-me-dev --local",
    "db:migrate:dev": "wrangler d1 migrations apply katasu-me-dev --remote",
    "db:migrate:prod": "wrangler d1 migrations apply katasu-me --remote --env production",
    "db:push": "wrangler d1 execute katasu-me-dev --local --file=$(ls ../../packages/service-db/src/migrations/*.sql | fzf)",
    "db:push:dev": "wrangler d1 execute katasu-me-dev --remote --file=$(ls ../../packages/service-db/src/migrations/*.sql | fzf)",
    "db:push:prod": "wrangler d1 execute katasu-me --remote --file=$(ls ../../packages/service-db/src/migrations/*.sql | fzf) --env production",
    "db:seed": "wrangler d1 execute katasu-me-dev --file=../../packages/service-db/src/seeds/plans.sql --local",
    "db:seed:dev": "wrangler d1 execute katasu-me-dev --file=../../packages/service-db/src/seeds/plans.sql",
    "db:seed:prod": "wrangler d1 execute katasu-me --remote --file=../../packages/service-db/src/seeds/plans.sql --env production"
  },
  "dependencies": {
    "@arrow2nd/vaul": "^1.1.7",
    "@conform-to/react": "^1.13.3",
    "@conform-to/valibot": "^1.13.3",
    "@katasu.me/service-db": "workspace:*",
    "@opennextjs/cloudflare": "^1.11.1",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-switch": "^1.2.6",
    "babel-plugin-react-compiler": "^19.1.0-rc.2",
    "better-auth": "^1.3.34",
    "budoux": "^0.7.0",
    "drizzle-orm": "^0.44.7",
    "hono": "^4.10.4",
    "image-size": "^2.0.2",
    "motion": "^12.23.24",
    "nanoid": "^5.1.6",
    "next": "16.0.1",
    "openai": "^6.8.1",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "react-easy-crop": "^5.5.3",
    "react-use": "^17.6.0",
    "tailwind-merge": "^3.4.0",
    "valibot": "^1.1.0"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.4",
    "@storybook/addon-docs": "^10.0.6",
    "@storybook/addon-links": "^10.0.6",
    "@storybook/addon-onboarding": "^10.0.6",
    "@storybook/nextjs": "^10.0.6",
    "@storybook/react": "^10.0.6",
    "@svgr/webpack": "^8.1.0",
    "@tailwindcss/postcss": "^4.1.17",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^24.10.0",
    "@types/react": "19.2.2",
    "@types/react-dom": "19.2.2",
    "@vitejs/plugin-react": "^5.1.0",
    "@vitest/ui": "^4.0.8",
    "better-sqlite3": "^12.4.1",
    "copy-webpack-plugin": "^13.0.1",
    "dotenv-cli": "^11.0.0",
    "drizzle-kit": "^0.31.6",
    "esbuild": "^0.27.0",
    "happy-dom": "^20.0.10",
    "playwright": "^1.56.1",
    "postcss": "^8.5.6",
    "storybook": "^10.0.6",
    "storybook-addon-pseudo-states": "^10.0.6",
    "tailwindcss": "^4.1.17",
    "typescript": "^5.9.3",
    "vite": "^7.2.2",
    "vite-plugin-svgr": "^4.5.0",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^4.0.8",
    "wrangler": "^4.46.0"
  }
}
