{
  "name": "@katasu.me/service",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite dev --port 3000",
    "build": "vite build",
    "serve": "vite preview",
    "test": "vitest run",
    "deploy": "pnpm run build && wrangler deploy",
    "preview": "pnpm run build && vite preview",
    "cf-typegen": "wrangler types",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "db:generate": "drizzle-kit generate --dialect sqlite --out ../../packages/service-db/src/migrations --schema ../../packages/service-db/src/schema",
    "db:studio": "dotenvx run -f .env.local -- drizzle-kit studio",
    "db:migrate": "wrangler d1 migrations apply katasu-me-dev --local",
    "db:migrate:dev": "wrangler d1 migrations apply katasu-me-dev --remote",
    "db:migrate:prod": "wrangler d1 migrations apply katasu-me --remote --env production",
    "db:push": "wrangler d1 execute katasu-me-dev --local --file=$(ls ../../packages/service-db/src/migrations/*.sql | fzf)",
    "db:push:dev": "wrangler d1 execute katasu-me-dev --remote --file=$(ls ../../packages/service-db/src/migrations/*.sql | fzf)",
    "db:push:prod": "wrangler d1 execute katasu-me --remote --file=$(ls ../../packages/service-db/src/migrations/*.sql | fzf) --env production",
    "db:seed": "wrangler d1 execute katasu-me-dev --file=../../packages/service-db/src/seeds/plans.sql --local",
    "db:seed:dev": "wrangler d1 execute katasu-me-dev --file=../../packages/service-db/src/seeds/plans.sql",
    "db:seed:prod": "wrangler d1 execute katasu-me --remote --file=../../packages/service-db/src/seeds/plans.sql --env production"
  },
  "dependencies": {
    "@arrow2nd/vaul": "^1.1.7",
    "@cloudflare/vite-plugin": "^1.16.0",
    "@katasu.me/service-db": "workspace:*",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-slot": "^1.2.4",
    "@tailwindcss/vite": "^4.0.6",
    "@tanstack/react-devtools": "^0.8.2",
    "@tanstack/react-form": "^1.27.0",
    "@tanstack/react-query": "^5.90.11",
    "@tanstack/react-router": "^1.139.14",
    "@tanstack/react-router-devtools": "^1.139.14",
    "@tanstack/react-router-ssr-query": "^1.139.14",
    "@tanstack/react-start": "^1.139.14",
    "@tanstack/router-plugin": "^1.139.14",
    "better-auth": "^1.4.2",
    "budoux": "^0.7.0",
    "drizzle-kit": "^0.31.6",
    "drizzle-orm": "^0.44.7",
    "image-size": "^2.0.2",
    "motion": "^12.23.24",
    "nanoid": "^5.1.6",
    "openai": "^6.9.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-easy-crop": "^5.5.5",
    "tailwind-merge": "^3.4.0",
    "tailwindcss": "^4.0.6",
    "valibot": "^1.2.0",
    "vite-tsconfig-paths": "^5.1.4",
    "wasm-image-optimization": "^1.4.3"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.8",
    "@dotenvx/dotenvx": "^1.51.1",
    "@fontsource-variable/reddit-sans": "^5.2.7",
    "@fontsource/ibm-plex-sans-jp": "^5.2.8",
    "@storybook/addon-docs": "^10.0.6",
    "@storybook/addon-links": "^10.0.6",
    "@storybook/react": "^10.0.6",
    "@storybook/react-vite": "^10.0.6",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.2.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^22.19.0",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "@vitest/ui": "^4.0.15",
    "better-sqlite3": "^12.5.0",
    "happy-dom": "^20.0.10",
    "storybook": "^10.0.6",
    "storybook-addon-pseudo-states": "^10.0.6",
    "typescript": "^5.7.2",
    "vite": "^7.1.7",
    "vite-plugin-svgr": "^4.5.0",
    "vitest": "^4.0.15",
    "web-vitals": "^5.1.0",
    "wrangler": "^4.52.0"
  }
}
