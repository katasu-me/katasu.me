{
  "name": "katasu.me",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack --experimental-https -H local.katasu.me",
    "build": "next build",
    "build:worker": "opennextjs-cloudflare build",
    "start": "next start",
    "preview": "opennextjs-cloudflare build && opennextjs-cloudflare preview",
    "cf-typegen": "wrangler types --env-interface CloudflareEnv ./cloudflare-env.d.ts",
    "lint": "biome lint --write ./src/**/*",
    "format": "biome format ./src/**/* --write",
    "check": "biome check --write ./src/**/*",
    "test": "vitest run",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "db:generate": "drizzle-kit generate --dialect sqlite --out ./src/db/migrations --schema ./src/db/schema",
    "db:studio": "bun --env-file=.env.local run drizzle-kit studio",
    "db:migrate": "wrangler d1 migrations apply katasu-me-dev",
    "db:migrate:local": "wrangler d1 migrations apply katasu-me-dev --local",
    "db:migrate:prod": "wrangler d1 migrations apply katasu-me",
    "db:push": "wrangler d1 execute katasu-me-dev --remote --file=$(ls ./src/db/migrations/*.sql | fzf)",
    "db:push:prod": "wrangler d1 execute katasu-me --remote --file=$(ls ./src/db/migrations/*.sql | fzf)"
  },
  "dependencies": {
    "@opennextjs/cloudflare": "^1.6.5",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.6",
    "babel-plugin-react-compiler": "^19.1.0-rc.2",
    "better-auth": "^1.3.7",
    "budoux": "^0.7.0",
    "drizzle-orm": "^0.44.4",
    "motion": "^12.23.12",
    "nanoid": "^5.1.5",
    "next": "15.5.0",
    "react": "19.1.1",
    "react-dom": "19.1.1",
    "react-use": "^17.6.0",
    "tailwind-merge": "^3.3.1",
    "vaul": "^1.1.2",
    "zod": "^4.1.1"
  },
  "devDependencies": {
    "@biomejs/biome": "2.0.0",
    "@storybook/addon-docs": "^9.1.3",
    "@storybook/addon-links": "^9.1.3",
    "@storybook/addon-onboarding": "^9.1.3",
    "@storybook/nextjs": "^9.1.3",
    "@svgr/webpack": "^8.1.0",
    "@tailwindcss/postcss": "^4.1.12",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/bun": "^1.2.20",
    "@types/node": "^22.17.2",
    "@types/react": "19.1.11",
    "@types/react-dom": "19.1.7",
    "@vitejs/plugin-react": "^5.0.1",
    "@vitest/coverage-v8": "3.2.4",
    "drizzle-kit": "^0.31.4",
    "esbuild": "^0.25.9",
    "jsdom": "^26.1.0",
    "lefthook": "^1.12.3",
    "postcss": "^8.5.6",
    "storybook": "^9.1.3",
    "storybook-addon-pseudo-states": "^9.1.3",
    "tailwindcss": "^4.1.12",
    "typescript": "^5.9.2",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4",
    "wrangler": "^4.32.0"
  },
  "imports": {
    "#*": [
      "./*",
      "./*.ts",
      "./*.tsx"
    ]
  },
  "overrides": {
    "@types/react": "19.1.11",
    "@types/react-dom": "19.1.7"
  }
}
